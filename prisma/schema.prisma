// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
   output   = "./generated"
}

datasource db {
  provider = "sqlite"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String
  service   String?
  comment   String?
  route     String?
  createdAt DateTime @default(now())
}